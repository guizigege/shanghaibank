    "http-proxy-middleware": "^0.18.0",
    "husky": "^1.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "mpvue-entry": "^1.4.7",
    "mpvue-loader": "^1.1.4",
    "mpvue-template-compiler": "^1.0.13",
    "mpvue-webpack-target": "^1.0.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.0.0",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.1.4",
    "postcss-mpvue-wxss": "^1.0.0",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "px2rpx-loader": "^0.1.10",
    "relative": "^3.0.2",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.8.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "tencent-wx-app": "^1.1.201712221707",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-style-loader": "^4.1.0",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-middleware-hard-disk": "^1.12.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0",
    "webpack-mpvue-asset-plugin": "^0.1.1",
    "webpack-mpvue-vendor-plugin": "^0.0.1"
  },
  "lint-staged": {
    "/src/**/*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "/src/**/*.ts": [
      "npm run lint:ts",
      "git add"
    ],
    "/src/**/*.js": [
      "npm run lint:es",
      "git add"
    ],
    "/src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
