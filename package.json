{
  "name": "xn-hk-web",
  "version": "1.0.0",
  "description": "xn-hk-web",
  "author": "shenghui <shenghui@shubidatech.com>",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "dev:wx": "node build/dev-server.js client wx",
    "start:wx": "npm run dev:wx",
    "build:wx": "node build/build.js client wx",
    "dev:swan": "node build/dev-server.js client swan",
    "start:swan": "npm run dev:swan",
    "build:swan": "node build/build.js client swan",
    "dev": "node build/dev-server.js client wx",
    "start": "npm run dev",
    "start:admin": "node build/dev-server.js admin wx",
    "build": "node build/build.js client wx",
    "build:admin": "node build/build.js admin wx",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:style",
    "lint:es": "eslint --fix --ext .js,.vue src",
    "lint:ts": "tslint --fix --project tsconfig.json -c tslint.json --format codeFrame",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "flyio": "^0.6.6",
    "mpvue": "^1.0.13",
    "vue": "^2.5.17",
    "vue-property-decorator": "^7.3.0",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "weapp-cookie": "^1.2.6"
  },
  "devDependencies": {
    "@types/vuelidate": "^0.7.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.4.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
